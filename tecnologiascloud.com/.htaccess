RewriteEngine On
	RewriteCond %{HTTP_HOST} ^(mail\.)?tecnologiascloud.com$ [NC]
	RewriteRule "(.*)" "http://tecnocloud.awsapps.com/mail" [R=301,L]

# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>

# END WordPress